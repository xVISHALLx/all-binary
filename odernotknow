#include <bits/stdc++.h>
using namespace std;

#define fast ios::sync_with_stdio(0); cin.tie(0);
#define ll long long
#define all(x) (x).begin(), (x).end()

int binarysearch(vector<int>&v,int n,int key){
	int low ,high ,mid;
	low=0;
	high=n-1;
	bool isascending=v[low]<v[high];
	while(low<=high){
		mid=low+(high -low)/2;
		if(key==v[mid]){
			return mid;
		}
		if(isascending){
			if(key<v[mid]){
				high=mid-1;
			}
			else{
				low=mid+1;
			}
		}
		else{
			if(key>v[mid]){
				high=mid-1;
			}
			else{
				low=mid+1;
			}
		}
	}
	return -1;

}

int main() {
    int n, key=6;
    cin>>n;
    vector<int> v(n);
    for(int i=0;i<n;i++){
    cin>>v[i];
    }
    cout<<binarysearch(v,n,key);

    return 0;
}
