#include<bits/stdc++.h>
using namespace std;
 void print( vector<int> &vec, int n){
 	for(int i: vec){
 		cout<< i <<" ";
 	}
 }
 int firstoc(vector<int> &vec, int n ,int key){
 	int low,mid,high;
 	low=0;
 	high=n-1;
 	int result=-1;
 	while(low<high){
 		mid=low+(high-low)/2;
 		if(key==vec[mid]){
 			result=mid;
 			return result;
 		}
 		if(key<mid){
 			high=mid-1;
 		}
 		else{
 			low=mid+1;
 		}

 	}
return -1;

 }
int main(){
	#ifndef ONLINE_JUDGE
	freopen("input.txt","r",stdin);
	freopen("output.txt","w",stdout);
	#endif
	int n,key =10; cin>>n;
vector<int> vec(n);
for(int i=0;i<n;i++){
	cin>>vec[i];
}
cout<<firstoc(vec,n,key);

}
